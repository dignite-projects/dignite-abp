@using Volo.Abp.Ui.Branding
@inject IBrandingProvider BrandingProvider
<a href="~/" class="navbar-brand d-flex align-items-center fs-4">
    @if (!BrandingProvider.LogoUrl.IsNullOrWhiteSpace())
    {
        <img src="@BrandingProvider.LogoUrl" alt="@BrandingProvider.AppName" class="d-inline-block align-text-top">
    }
    @BrandingProvider.AppName
</a>
